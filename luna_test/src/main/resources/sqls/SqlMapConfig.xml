<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>


	
<!-- 	변수설정 -->
	<typeAliases>
		<typeAlias type="com.luna.board.dtos.MemberDTO" alias="MDTO"/>
		<typeAlias type="com.luna.board.dtos.PBoardDTO" alias="PBDTO"/>
		<typeAlias type="com.luna.board.dtos.RBoardDTO" alias="RBDTO"/>
	</typeAliases>
<!-- 	DB접속 정보 셋팅 -->	
	<mappers>
		<mapper resource="./sqls/PboardDataMapper.xml"/>
		<mapper resource="./sqls/StockDataMapper.xml"/>
		<mapper resource="./sqls/MemberDataMapper.xml"/>
		<mapper resource="./sqls/RboardDataMapper.xml"/>
	</mappers>
</configuration>



	
	